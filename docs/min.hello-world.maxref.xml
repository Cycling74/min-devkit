<c74object name='min.hello-world' category='utilities'>

	<digest>Post to the Max Console</digest>
	<description>Post to the Max Console. </description>


	<!--METADATA-->

	<metadatalist>
		<metadata name='author'>Cycling '74 </metadata>
		<metadata name='tag'>utilities</metadata>
	</metadatalist>


	<!--ARGUMENTS-->

	<objarglist>

		<objarg name='greeting' optional='1' type='number'>
			<digest>Initial value for the greeting attribute</digest>
			<description>Initial value for the greeting attribute. </description>
		</objarg>

	</objarglist>


	<!--MESSAGES-->

	<methodlist>

		<method name='bang'>
			<digest>Post the greeting</digest>
			<description>Post the greeting. </description>
		</method>

	</methodlist>


	<!--ATTRIBUTES-->

	<attributelist>

		<attribute name='greeting' get='1' set='1' type='symbol' size='1' >
			<digest>Greeting to be posted</digest>
			<description>Greeting to be posted. The greeting will be posted to the Max console when a bang is received. </description>
		</attribute>

	</attributelist>


	<!--RELATED-->

	<seealsolist>
		<seealso name='print' />
		<seealso name='jit.print' />
		<seealso name='dict.print' />
	</seealsolist>


</c74object>
